{
  "name": "tazta2ra.github.io",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "ELEVENTY_ENV=development eleventy",
    "start": "npm run watch:eleventy & npm run watch:sass",
    "build": "npm run build:eleventy & npm run build:sass",
    "watch:sass": "sass --load-path=node_modules --no-source-map sass/tazta2ra.scss src/css/tazta2ra.css --watch",
    "build:sass": "sass --load-path=node_modules --no-source-map sass/tazta2ra.scss src/css/tazta2ra.css",
    "build:sass:prod": "sass --no-source-map sass/tazta2ra.scss src/css/tazta2ra.css --style compressed",
    "build:eleventy:prod": "ELEVENTY_ENV=production eleventy",
    "build:prod": "npm run build:eleventy:prod & npm run build:sass:prod"
  },
  "keywords": [
    "beyond",
    "tazta2ra",
    "aninuscsalas",
    "bluelotus"
  ],
  "author": "tazta2ra",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "bulma": "^1.0.1",
    "sass": "^1.77.6"
  }
}
